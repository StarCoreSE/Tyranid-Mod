    <Definition xsi:type="MyObjectBuilder_ConveyorSorterDefinition">
      <!-- Heavy Gimbaled Cannon Emplacment -->
      <Id>
        <TypeId>ConveyorSorter</TypeId>
        <SubtypeId>NID_Pyroacid</SubtypeId>
      </Id>
      <DisplayName>Pyro-acid Cannon</DisplayName>
      <Description>
        Large Biological Gimbaled Cannon
        Will melt armor away, while leaving components intact
        Medium Shield Damage
        4km Effective Range
      </Description>
      <Icon>Textures\GUI\Icons\Cubes\Pyroacid_Icon.png</Icon>
      <CubeSize>Large</CubeSize>
      <GuiVisible>true</GuiVisible>
      <BlockTopology>TriangleMesh</BlockTopology>
      <Size x="3" y="3" z="7" />
      <ModelOffset x="0" y="0" z="0" />
      <Model>Models\Cubes\Pyroacid Cannon.mwm</Model>
      <UseModelIntersection>true</UseModelIntersection>
      <Components>
        <Component Subtype="SteelPlate" Count="500" />
        <Component Subtype="Superconductor" Count="300" />
        <Component Subtype="Construction" Count="200" />
        <Component Subtype="Motor" Count="245" />
        <Component Subtype="Reactor" Count="210" />
        <Component Subtype="Detector" Count="90" />
        <Component Subtype="Computer" Count="400" />
        <Component Subtype="MetalGrid" Count="250" />
        <Component Subtype="SteelPlate" Count="3500" />
      </Components>
      <CriticalComponent Subtype="Computer" Index="0" />
      <MountPoints>
        <MountPoint Side="Back" StartX="1" StartY="1" EndX="3" EndY="3" />
      </MountPoints>
      <VoxelPlacement>
        <!--Possible settings Both,InVoxel,OutsideVoxel,Volumetric. If volumetric set than MaxAllowed and MinAllowed will be used.-->
        <StaticMode>
          <PlacementMode>OutsideVoxel</PlacementMode>
          <MaxAllowed>0.2</MaxAllowed>
          <MinAllowed>0</MinAllowed>
        </StaticMode>
        <DynamicMode>
          <PlacementMode>OutsideVoxel</PlacementMode>
          <MaxAllowed>0.2</MaxAllowed>
          <MinAllowed>0.01</MinAllowed>
        </DynamicMode>
      </VoxelPlacement>
      <BlockPairName>NID_Pyroacid</BlockPairName>
      <MirroringY>Z</MirroringY>
      <MirroringZ>Y</MirroringZ>
      <EdgeType>Light</EdgeType>
      <BuildTimeSeconds>120</BuildTimeSeconds>
      <OverlayTexture>Textures\GUI\Screens\K_camera_overlay_1.dds</OverlayTexture>
      <WeaponDefinitionId Subtype="K_HSR_Gun"/>
      <InventoryMaxVolume>1</InventoryMaxVolume>
      <DamageEffectId>213</DamageEffectId>
      <AiEnabled>false</AiEnabled>
      <DamagedSound>ParticleWeapExpl</DamagedSound>
      <ResourceSinkGroup>Defense</ResourceSinkGroup>
      <MinElevationDegrees>-3.5</MinElevationDegrees>
      <MaxElevationDegrees>3.5</MaxElevationDegrees>
      <MinAzimuthDegrees>-3.5</MinAzimuthDegrees>
      <MaxAzimuthDegrees>3.5</MaxAzimuthDegrees>
      <RotationSpeed>0.0001</RotationSpeed>
      <ElevationSpeed>0.0001 </ElevationSpeed>
      <IdleRotation>false</IdleRotation>
      <MaxRangeMeters>4000</MaxRangeMeters>
      <MinFov>0.0001</MinFov>
      <MaxFov>2</MaxFov>
      <EmissiveColorPreset>Default</EmissiveColorPreset>
    </Definition>


